{
  "_id": "difi-weather-forecast",
  "type": "pipe",
  "source": {
    "type": "conditional",
    "alternatives": {
      "prod": {
        "type": "dataset",
        "dataset": "global-location"
      },
      "test": {
        "type": "embedded",
        "entities": [{}]
      }
    },
    "condition": "$ENV(node-env)"
  },
  "transform": {
    "type": "conditional",
    "alternatives": {
      "prod": {
        "type": "chained",
        "transforms": [{
          "type": "dtl",
          "rules": {
            "default": [
              ["copy", "*"],
              ["filter",
                ["eq", "_S.postnummer", "8005"]
              ]
            ]
          }
        }, {
          "type": "http",
          "system": "weather-forecast",
          "batch_size": 1,
          "url": "/get"
        }, {
          "type": "dtl",
          "rules": {
            "default": [
              ["add", "_id", "_S._id"],
              ["copy", "*"]
            ]
          }
        }]
      },
      "test": {
        "type": "dtl",
        "rules": {
          "default": [
            ["copy", "*"]
          ]
        }
      }
    },
    "condition": "$ENV(node-env)"
  },
  "pump": {
    "mode": "manual"
  },
  "namespaced_identifiers": true
}
